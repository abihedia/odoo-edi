<?xml version="1.0"?>
<odoo>
  <data>

    <!-- Create "EDI Stock Level Report Document" sequence -->
    <record id="quant_report_sequence_default" model="ir.sequence">
      <field name="name">EDI Stock Level Report Document</field>
      <field name="prefix">STK</field>
      <field name="padding" eval="5"/>
    </record>

    <!-- EDI stock level report record type -->
    <record id="quant_report_record_type" model="edi.record.type">
      <field name="name">Stock Level Report</field>
      <field name="model_id" ref="model_edi_quant_report_record"/>
      <field name="sequence">70</field>
    </record>

  </data>
</odoo>
