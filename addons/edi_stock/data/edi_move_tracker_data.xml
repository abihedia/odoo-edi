<?xml version="1.0"?>
<odoo>
  <data>

    <!-- Create "EDI Stock Move Tracker Document" sequence -->
    <record id="move_tracker_sequence_default" model="ir.sequence">
      <field name="name">EDI Stock Move Tracker Document</field>
      <field name="prefix">TRK</field>
      <field name="padding" eval="5"/>
    </record>

    <!-- EDI stock move tracker record type -->
    <record id="move_tracker_record_type" model="edi.record.type">
      <field name="name">Stock Move Tracker</field>
      <field name="model_id" ref="model_edi_move_tracker_record"/>
      <field name="sequence">30</field>
    </record>

  </data>
</odoo>
